{
    "cli": {
        "version": ">= 13.4.2",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "distribution": "internal",
            "channel": "preview"
        },
        "preview1": {
            "android": {
                "gradleCommand": ":app:assembleRelease"
            },
            "channel": "preview1"
        },
        "preview2": {
            "android": {
                "gradleCommand": ":app:assembleRelease"
            },
            "channel": "preview2"
        },
        "preview3": {
            "developmentClient": true,
            "channel": "preview3"
        },
        "preview4": {
            "distribution": "internal",
            "channel": "preview4"
        },
        "production": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            },
            "distribution": "store",
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
